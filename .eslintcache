item\">\r\n              <Link\r\n                style={currentTab(history, \"/cart\")}\r\n                className=\"nav-link\"\r\n                to=\"/cart\"\r\n              >\r\n                <i className=\"fas fa-shopping-cart pr-1\"></i> Cart{\" \"}\r\n                {productsInCart > 0 ? (\r\n                  <span class=\"badge badge-pill badge-warning\">\r\n                    {productsInCart}\r\n                  </span>\r\n                ) : (\r\n                  \"\"\r\n                )}\r\n              </Link>\r\n            </li>\r\n            {isAuthenticated() && isAuthenticated().user.role === 0 && (\r\n              <li className=\"nav-item\">\r\n                <Link\r\n                  style={currentTab(history, \"/user/dashboard\")}\r\n                  className=\"nav-link\"\r\n                  to=\"/user/dashboard\"\r\n                >\r\n                  <i className=\"fas fa-chalkboard pr-1\"></i> Dashboard\r\n                </Link>\r\n              </li>\r\n            )}\r\n            {isAuthenticated() && isAuthenticated().user.role === 1 && (\r\n              <li className=\"nav-item\">\r\n                <Link\r\n                  style={currentTab(history, \"/admin/dashboard\")}\r\n                  className=\"nav-link\"\r\n                  to=\"/admin/dashboard\"\r\n                >\r\n                  <i className=\"fas fa-chalkboard pr-1\"></i> Dashboard\r\n                </Link>\r\n              </li>\r\n            )}\r\n            {!isAuthenticated() && (\r\n              <>\r\n                <li className=\"nav-item\">\r\n                  <Link\r\n                    style={currentTab(history, \"/signup\")}\r\n                    className=\"nav-link\"\r\n                    to=\"/signup\"\r\n                  >\r\n                    <i className=\"fas fa-user-plus pr-1\"></i> Signup\r\n                  </Link>\r\n                </li>\r\n                <li className=\"nav-item\">\r\n                  <Link\r\n                    style={currentTab(history, \"/signin\")}\r\n                    className=\"nav-link\"\r\n                    to=\"/signin\"\r\n                  >\r\n                    <i className=\"fas fa-sign-in-alt pr-1\"></i> Sign In\r\n                  </Link>\r\n                </li>\r\n              </>\r\n            )}\r\n            {isAuthenticated() && (\r\n              <li className=\"nav-item\">\r\n                {/* <Link\r\n            style={currentTab(history, \"/signout\")}\r\n            className=\"nav-link\"\r\n            to=\"/signout\"\r\n          >\r\n            Signout\r\n          </Link> */}\r\n                <span\r\n                  className=\"nav-link text-warning\"\r\n                  style={{ cursor: \"pointer\" }}\r\n                  onClick={() => {\r\n                    signout(() => {\r\n                      history.push(\"/\");\r\n                    });\r\n                  }}\r\n                >\r\n                  Signout <i className=\"fas fa-sign-out-alt\"></i>\r\n                </span>\r\n              </li>\r\n            )}\r\n            <li className=\"nav-item nav-link text-white d-none d-md-block\">\r\n              |\r\n            </li>\r\n            <li className=\"nav-item\">\r\n              <a\r\n                href=\"https://github.com/ArijitCodes\"\r\n                target=\"_blank\"\r\n                className=\"nav-link text-white\"\r\n              >\r\n                <i className=\"fab fa-github pr-1\"></i> GitHub\r\n                <i className=\"fas fa-external-link-alt pl-1\"></i>\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </nav>\r\n      {/* </div>\r\n  </div>  */}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default withRouter(Menu);\r\n","F:\\Development\\mern-stack\\ecom\\projfrontend\\src\\user\\Signup.js",[],"F:\\Development\\mern-stack\\ecom\\projfrontend\\src\\user\\Signin.js",["183"],"import React, { useState } from