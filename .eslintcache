m\";\r\n\r\n// Components\r\nimport { isAuthenticated, signout } from \"../auth/helper\";\r\nimport { checkCart } from \"./helper/cartHelper\";\r\n\r\n// Context\r\nimport CartContext from \"./context/cartNotification/cartContext\";\r\n\r\nconst currentTab = (history, path) => {\r\n  if (history.location.pathname === path) {\r\n    return { color: \"#2ecc72\" };\r\n  } else {\r\n    return { color: \"#FFFFFF\" };\r\n  }\r\n};\r\n\r\nconst Menu = ({ history }) => {\r\n  const cartContext = useContext(CartContext);\r\n  const { productsInCart, setProductsInCart } = cartContext;\r\n\r\n  // Setting No. of Products inside Cart into Global Cart Notification Context\r\n  useEffect(() => {\r\n    setProductsInCart(checkCart());\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      {/* <div className=\"row\">\r\n    <div className=\"col-12\"> */}\r\n      <nav className=\"navbar navbar-expand-md navbar-dark bg-dark sticky-top\">\r\n        <Link to=\"/\" className=\"nav-link pl-0\" style={currentTab(history, \"/\")}>\r\n          <h4 className=\"my-auto text-white\">\r\n            <i className=\"fas fa-laptop-code rounded p-2 mr-2 bg-info\"></i>\r\n            The NerdStore\r\n          </h4>\r\n        </Link>\r\n        <button\r\n          className=\"navbar-toggler\"\r\n          type=\"button\"\r\n          data-toggle=\"collapse\"\r\n          data-target=\"#navbarSupportedContent\"\r\n          aria-controls=\"navbarSupportedContent\"\r\n          aria-expanded=\"false\"\r\n          aria-label=\"Toggle navigation\"\r\n        >\r\n          <span className=\"navbar-toggler-icon\"></span>\r\n        </button>\r\n        <div className=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\r\n          {/* <ul className=\"nav nav-tabs bg-dark\"> */}\r\n          <ul className=\"navbar-nav ml-auto my-auto\">\r\n            <li className=\"nav-item\">\r\n              <Link\r\n                style={currentTab(history, \"/\")}\r\n                className=\"nav-link\"\r\n                to=\"/\"\r\n              >\r\n                <i className=\"fas fa-home pr-1\"></i> Home\r\n              </Link>\r\n            </li>\r\n            <li className=\"nav-item\">\r\n              <Link\r\n                style={currentTab(history, \"/cart\")}\r\n                className=\"nav-link\"\r\n                to=\"/cart\"\r\n              >\r\n                <i className=\"fas fa-shopping-cart pr-1\"></i> Cart{\" \"}\r\n                {productsInCart > 0 ? (\r\n                  <span class=\"badge badge-pill badge-warning\">\r\n                    {productsInCart}\r\n                  </span>\r\n                ) : (\r\n                  \"\"\r\n                )}\r\n              </Link>\r\n            </li>\r\n            {isAuthenticated() && isAuthenticated().user.role === 0 && (\r\n              <li className=\"nav-item\">\r\n                <Link\r\n                  style={currentTab(history, \"/user/dashboard\")}\r\n                  className=\"nav-link\"\r\n                  to=\"/user/dashboard\"\r\n                >\r\n                  <i className=\"fas fa-chalkboard pr-1\"></i> Dashboard\r\n                </Link>\r\n              </li>\r\n            )}\r\n            {isAuthenticated() && isAuthenticated().user.role === 1 && (\r\n              <li className=\"nav-item\">\r\n                <Link\r\n                  style={currentTab(history, \"/admin/dashboard\")}\r\n                  className=\"nav-link\"\r\n                  to=\"/admin/dashboard\"\r\n                >\r\n                  <i className=\"fas fa-chalkboard pr-1\"></i> Dashboard\r\n                </Link>\r\n              </li>\r\n            )}\r\n            {!isAuthenticated() && (\r\n              <>\r\n                <li className=\"nav-item\">\r\n                  <Link\r\n                    style={currentTab(history, \"/signup\")}\r\n                    className=\"nav-link\"\r\n                    to=\"/signup\"\r\n                  >\r\